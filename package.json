    "dotenv": "^8.2.0",
    "echarts": "^4.9.0",
    "electron-is-packaged": "^1.0.2",
    "electron-json-storage": "^4.3.0",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5",
    "express": "^4.17.1",
    "got": "^11.7.0",
